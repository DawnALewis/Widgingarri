RUNFOLDER="/gpfs/users/a1867445/Demu/241220_INO_BLlDLeVPeYSoSRa_WidgingarriFitzroyCrossingNewguinea2DingoShotgun"
SAMPLESHEET="/gpfs/users/a1867445/Demu/SampleSheet_Widg.csv"
OUTPUT_DIR="/gpfs/users/a1867445/Demu/Widg_demux_13Feb2025"
MASKING="Y*,I7N3,N*,Y*"

sbatch /hpcfs/groups/acad_users/shyrav/resources/bcl2fastq.sh -r ${RUNFOLDER} -s ${SAMPLESHEET} -o ${OUTPUT_DIR} -m ${MASKING}



## Demux with individual library indexes

concatenate lanes
cat Widg_GAII_15_S1_L001_R1_001.fastq.gz Widg_GAII_15_S1_L002_R1_001.fastq.gz > Widg_GAII_15_S1_R1.fastq.gz
cat Widg_GAII_15_S1_L001_R2_001.fastq.gz Widg_GAII_15_S1_L002_R2_001.fastq.gz > Widg_GAII_15_S1_R2.fastq.gz

ml Singularity
singularity exec -B /gpfs/ /hpcfs/groups/acad_users/shyrav/resources/singularity/nf-core-eager_2.4.5-sharding.sif \
AdapterRemoval \
--barcode-list ./Barcode_list.tsv \
--adapter1 AGATCGGAAGAGCACACGTCTGAACTCCAGTCACNNNNNNATCTCGTATGCCGTCTTCTGCTTG \
 --adapter2 AGATCGGAAGAGCGTCGTGTAGGGAAAGAGTGTAGATCTCGGTGGTCGCCGTATCATT \
 --file1 ./Widg_GAII_15_S1_R1.fastq.gz \
 --file2 ./Widg_GAII_15_S1_R2.fastq.gz \
 --basename Shotgun2025_ \
 --barcode-mm-r1 1 \
 --barcode-mm-r2 1 \
 --threads 20 \
 --demultiplex-only \
 --gzip

